<?xml version="0.0" ?>
<robot name="disk.urdf">
  <link name="baseLink">
    <contact>
      <lateral_friction value="1.0"/>
      <inertia_scaling value="3.0"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value="1.0"/>
       <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
			<cylinder length="0.5" radius="2.0"/>
      </geometry>
       <material name="white">
        <color rgba="0.2 0.2 1 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
			<cylinder length="0.05" radius="0.5"/>
      </geometry>
    </collision>
  </link>
</robot>

